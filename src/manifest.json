{
  "package": "com.qiyunjbcl.walkcloudqy",
  "name": "琦云计步",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.app"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "off",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Demo",
    "pages": {
      "pages/Demo": {
        "component": "index"
      },
      "pages/DemoDetail": {
        "component": "index"
      },
      "pages/Web": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "Demo",
      "resource": "pages/Demo"
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Demo": {
        "menu": false,
        "titleBar": false
      },
      "pages/DemoDetail": {
        "titleBarText": "详情页"
      },
      "pages/Web": {
        "titleBarText": "协议"
      }
    }
  },
  "deviceTypeList": ["phone"]
}
